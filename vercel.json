{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@now/node"
      }
    ],
    "routes": [
      {
        "src": "/api/routes/(.*)",
        "dest": "/api/routes/index.js"
      },
      {
        "src": "/api/_models/(.*)",
        "dest": "/api/_models/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ]
  }
  